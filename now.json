{
  "version": 2,
  "alias": "beam-calculator.now.sh",
  "routes": [
    {
      "src": "/img/(.*)",
      "headers": { "cache-control": "max-age=31104000, s-maxage=31104000" },
      "dest": "/img/$1"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "max-age=31104000, s-maxage=31104000" },
      "dest": "/$1"
    }
  ]
}
